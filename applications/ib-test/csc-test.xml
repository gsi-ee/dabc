<?xml version="1.0"?>
<dabc version="1">
  <Context host="node9-001" port="5432"/>
  <Context host="node1-001" port="5433"/>
  <Context host="node1-002" port="5432"/>
  <Context host="node1-003" port="5433"/>
  <Context host="node1-004" port="5432"/>
  <Defaults>
     <Context name="*">
       <Run>
         <lib value="libDabcIbTest.so"/>
         <LD_LIBRARY_PATH value="."/>
         <logfile value="/tmp/Node${DABCNODEID}.log"/>
         <debuglevel value="1"/>
	      <debugger value="false"/>
         <loglevel value="1"/>
         <loglimit value="1000000"/>
         <runtime value="50"/>
       </Run>
       <Application class="IbTestApp">
         <NetDevice value="dabc::SocketDevice"/>
         <BufferSize value="80000"/>

      <!-- allowed: SingleRoute, TimeSync or NormalTest -->
         <TestKind value="NormalTest"/>
         <TestNumLids value="16"/>
         <TestPattern value="0"/>
         <TestTime value="10"/>
         <TestRate value="2000"/>
         <TestBufferSize value="500000"/>
         <TestPoolSize value="250"/>
         <TestOutputQueue value="2"/>
         <TestInputQueue value="4"/>
         <TestSchedule value=""/>
         <TestTimeout value="20"/>
       </Application>
     </Context>
  </Defaults>
</dabc>
