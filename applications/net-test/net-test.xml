<?xml version="1.0"?>
<dabc version="1">
  <Context host="lxi013"/>
  <Context host="lxi010"/>
  <Context host="lxi011"/>
  <Context host="lxi012"/>
  <Defaults>
     <Context name="*">
       <Run>
         <lib value="libDabcNetTest.so"/>
         <debuglevel value="0"/>
<!--         <debugger value="gdb -q -x gdb.cmd --args"/>  -->
         <loglevel value="3"/>
         <logfile value="${Context}.log"/>
         <loglimit value="10000000000"/>
         <runfunc value="RunAllToAll"/>
         <sockethost value="${host}"/>
         <copycfg value="false"/>
       </Run>
       <User>
         <NetDevice value="dabc::SocketDevice"/>
         <BufferSize value="8000"/>
         <OutputQueueSize value="4"/>
         <InputQueueSize value="8"/>
         <UseAcknowledge value="0"/>
       </User>
     </Context>
  </Defaults>
</dabc>
