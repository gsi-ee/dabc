<?xml version="1.0"?>
<dabc version="2">
   <Context host="node03" port="5435"/>
   <Context host="node02" port="5434"/>
   <Context host="node01" port="5433"/>
   <Context host="master" port="5432"/>
   <Context host="*">
     <Run>
       <lib value="libDabcVerbs.so"/>
       <lib value="libDabcBnet.so"/>
       <!--LD_LIBRARY_PATH value="/home/linev/workspace/dabc/applications/ib-test"/-->
       <logfile value="/tmp/Node${DABCNODEID}.log"/>
       <debuglevel value="0"/>
       <debugger value="false"/>
       <errout value="/dev/null"/>
       <nullout value="false"/>
       <num_special_thrds value="1"/>
       <taskset value="-c 0-1"/>
       <loglevel value="0"/>
       <loglimit value="10000000"/>
       <runtime value="50"/>
       <!--runfunc value="test_affinity"/-->
     </Run>
     <Application class="bnet::Application">
         <NetDevice value="dabc::SocketDevice"/>
         <BufferSize value="65536"/>
         <NumBuffers value="2000"/>
         <SpecialThread value="true"/>

      <!-- allowed: TimeSync or Normal -->
         <TestKind value="Normal"/>
         <TestReliable value="true"/>
         <TestNumLids value="1"/>
         <TestPattern value="0"/>
         <TestTime value="5.0"/>
         <TestRate value="200"/>
         <TestOutputQueue value="3"/>
         <TestInputQueue value="6"/>
         <TestEventQueue value="1000"/>
         <TestControlKind value="1"/>
         <TestStartTurns value="20"/>
         <TestSchedule value=""/>
         <TestTimeout value="10"/>

         <EventLifeTime value="3."/>
         <SubmitPreTime value="0.005"/>
         
       </Application>
   </Context>
</dabc>
