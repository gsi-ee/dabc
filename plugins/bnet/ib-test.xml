<?xml version="1.0"?>
<dabc version="2">
   <Context host="master" port="5432"/>
   <Context host="node01" port="5433"/>
   <Context host="node02" port="5434"/>
   <Context host="node03" port="5435"/>
   <Context host="*">
     <Run>
       <lib value="libDabcVerbs.so"/>
       <lib value="libDabcBnet.so"/>
       <!--LD_LIBRARY_PATH value="/home/linev/workspace/dabc/applications/ib-test"/-->
       <logfile value="Node${DABCNODEID}.log"/>
       <debuglevel value="0"/>
       <debugger value="false"/>
       <nullout value="false"/>
       <num_special_proc value="1"/>
       <taskset value="-c 0-1"/>
       <loglevel value="1"/>
       <loglimit value="1000000"/>
       <runtime value="100"/>
       <!--runfunc value="test_affinity"/-->
     </Run>
     <Application class="bnet::Application">
         <NetDevice value="dabc::SocketDevice"/>
         <BufferSize value="65536"/>
         <NumBuffers value="2000"/>
         <SpecialThread value="true"/>

      <!-- allowed: OnlyConnect, Simple, SimpleSync, TimeSync or Normal -->
         <TestKind value="OnlyConnect"/>
         <TestReliable value="true"/>
         <TestNumLids value="1"/>
         <TestPattern value="0"/>
         <TestTime value="5.0"/>
         <TestRate value="500"/>
         <TestOutputQueue value="3"/>
         <TestInputQueue value="6"/>
         <TestSchedule value=""/>
         <TestTimeout value="10"/>
       </Application>
   </Context>
</dabc>
