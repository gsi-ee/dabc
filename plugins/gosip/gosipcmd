#!/bin/bash
# use for debugging where gosipcmd not availible

if [[ "$1" == "--" ]]; then shift; fi

if [[ "$3" == "-r" ]]; then 
  if [[ "$4" == "-d" ]]; then
    numregs=$6
    for (( cnt=0; cnt<numregs; cnt++ )) ; do
       echo "reg $cnt=$((cnt*4))"
    done
  else
    echo $4;  
  fi
  exit;
fi


if [[ "$3" == "-w" ]]; then exit; fi

echo "wrong arguments $3" 1>&2

