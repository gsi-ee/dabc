<?xml version="1.0"?>
<!-- This is example how repeater for any kind of MBS server can be created. -->
<dabc version="2">
  <Context name="Repeater">
    <Run>
      <lib value="libDabcMbs.so"/>
      <runtime value="10"/>
    </Run>
    <MemoryPool name="Pool">
       <BufferSize value="65536"/>
       <NumBuffers value="100"/>
    </MemoryPool>
    <Module name="Repeater" class="mbs::TransmitterModule">
       <Reconnect value="true"/>
       <InputPort name="Input0" url="lmd://Generator?size=32&numsub=2&total=1000"/>
       <OutputPort name="Output0" url="lmd://test.lmd?maxsize=100"/>
     </Module>
  </Context>
</dabc>
