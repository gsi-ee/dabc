<?xml version="1.0"?>
<dabc version="2">
  <Context name="Gener" port="4444">
    <Run>
      <lib value="libDabcMbs.so"/>
      <lib value="libDabcHttp.so"/>
      <!--lib value="libDabcRoot.so"/-->
      <logfile value="Gener.log"/>
      <loglimit value="1000000"/>
      <control value="true"/>
    </Run>

    <HttpServer name="http">
       <enabled value="true"/>
       <port value="8090"/>
       <select value=""/>
       <top value="${Context}"/>
    </HttpServer>

    <RootSniffer name="ROOT">
       <enabled value="true"/>
    </RootSniffer>

    <MemoryPool name="Pool">
       <BufferSize value="65536"/>
       <NumBuffers value="100"/>
    </MemoryPool>
    
    <Module name="Multi" class="dabc::MultiplexerModule">
       <NumInputs value="1"/>
       <NumOutputs value="1"/>
       <InputPort name="Input0" url="lmd://Generator?size=32&numsub=2" queue="5" rate="GRate"/>
       <OutputPort name="Output0" url="mbs://Stream:6767" queue="5"/>
       <GRate width="5" prec="3" low="0" up="50" debug="1"/>
     </Module>
  </Context>
</dabc>
